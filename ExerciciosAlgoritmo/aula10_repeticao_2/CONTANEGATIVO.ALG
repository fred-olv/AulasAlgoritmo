algoritmo "contanegativo"
var
      cont,n,totn:inteiro
inicio
      cont<-1
      Repita
      Escreva("digite um numero: ")
      Leia (n)
      Se (n<0) entao
         totn <- totn +1)
      Fimse
      cont<-cont+1
      ate(cont>5)
      Escreva(" o tatal de numeros negativos é: ", totn)
fimalgoritmo